<VBox fx:id="featuredCourseSection" fx:controller="com.view.elements.Carousel" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1">
    <HBox spacing="20" alignment="CENTER">
        <Button fx:id="leftButton" styleClass="modern-button" style="-fx-min-width: 45; -fx-min-height: 45; -fx-max-width: 45; -fx-max-height: 45; -fx-padding: 0; -fx-border-radius: 22.5; -fx-background-radius: 22.5;-fx-effect: dropshadow(three-pass-box, rgba(0,0,0,0.1), 4, 0, 0, 0);" text="&lt;">
            <font>
                <Font name="Franklin Gothic Medium" size="20.0" />
            </font>
            <HBox.margin>
                <Insets top="50.0" />
            </HBox.margin>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <VBox fx:id="rigthContainer" alignment="CENTER">
        </VBox>
        <Button fx:id="rigthButton" styleClass="modern-button" style="-fx-min-width: 45; -fx-min-height: 45; -fx-max-width: 45; -fx-max-height: 45; -fx-padding: 0; -fx-background-radius: 22.5; -fx-border-radius: 22.5; -fx-effect: dropshadow(three-pass-box, rgba(0,0,0,0.1), 4, 0, 0, 0);" text="&gt;">
            <font>
                <Font name="Franklin Gothic Medium" size="20.0" />
            </font>
            <HBox.margin>
                <Insets top="50.0" />
            </HBox.margin>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Button>
        <HBox fx:id="indicator">
            <HBox.margin>
                <Insets top="140.0" />
            </HBox.margin>
        </HBox>
    </HBox>
</VBox>
